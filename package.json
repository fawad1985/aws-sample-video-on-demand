{
  "name": "aws-sample-video-on-demand",
  "version": "0.1.0",
  "bin": {
    "aws-sample-video-on-demand": "bin/aws-sample-video-on-demand.js"
  },
  "scripts": {
    "test": "jest",
    "install": "cd functions && npm install",
    "bootstrap:dev": "cdk bootstrap -v --profile {profile_name} --context stage=dev",
    "synth:dev": "npx cdk -v --profile {profile_name} synth sample-video-on-demand-dev",
    "diff:dev": "npx cdk -v --profile {profile_name} diff sample-video-on-demand-dev",
    "deploy:dev": "npm run synth:dev && npx cdk -v --profile {profile_name} deploy sample-video-on-demand-dev",
    "destroy:dev": "npx cdk -v --profile {profile_name} destroy sample-video-on-demand-dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "aws-cdk": "2.130.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.130.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "esbuild": "^0.19.10"
  }
}